{
  "extends": ["tslint:recommended", "tslint-eslint-rules", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "no-empty-interface": false,
    "ordered-imports": false,
    "interface-name": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "jsx-double"]
  }
}
